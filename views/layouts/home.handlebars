<main class="list-vacantes">
    <h2>Lista de vacantes</h2>

    {{#if vacantes.length}}
        {{#each vacantes}}
            <div class="vacante">
                <div class="caja">
                    <h3>{{empresa}}</h3>
                    <p class="puesto">{{titulo}}</p>
                </div>
                <div class="caja">
                    <p class="etiqueta">ubicación</p>
                    <p class="nombre">{{ubicacion}}</p>
                </div>
                <div class="caja">
                    <p class="etiqueta">contrato</p>
                    <p class="nombre contratro"> {{contrato }}</p>
                </div>
                <div class="caja central-vertical">
                    <a href="/vacantes/{{url}}" class="btn btn-verde"> Más Info</a>
                </div>
            </div>
        {{/each}}
        {{else}}
        <p>No hay Vacantes</p>
        {{/if}}
</main>